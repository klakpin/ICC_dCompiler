var size1:=10;
var arr:=[];
var var1:=-1;
for i in 0..size1
  loop
    arr[i]:=i*var1;
    var1:=var1*(-1);
  end

var selectionsort:=func(arr, size1)
  var min:=0;
  for i in 0..size1-1
    loop
      min:=i;
      for j in i+1..size1
        loop
          if arr[j]<arr[min] then min:=j; end
        end
      var temp:=arr[i];
      arr[i]:=arr[min];
      arr[min]:=temp;
    end

  print1(arr,size1);
  end

var print1:=func(arr, size1)
  for i in 0..size1
    loop
      print(arr[i]);
    end
  end

selectionsort(arr,size1);