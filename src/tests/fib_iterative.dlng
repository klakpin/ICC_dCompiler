var fib:= func(n)

        if n <= 1
        then
            return n;
        end

        var fib := 1;
        var prevFib := 1;

        for i in 2 .. n
        loop
          var temp := fib;
          fib:=fib+prevFib;
          prevFib := temp;
        end

        return fib;
    end
    print(fib(n))